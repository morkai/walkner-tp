<input id="<%- idPrefix %>-quantity" type="hidden" name="quantity" value="1">
<input id="<%- idPrefix %>-unit" type="hidden" name="unit" value="vehicle">
<div class="form-group">
  <label for="<%- idPrefix %>-userDate" class="control-label is-required"><%- t('transportOrders', 'userDate:carWash') %></label>
  <div class="form-group-datetime">
    <input id="<%- idPrefix %>-userDate" class="form-control" type="date" name="userDate" required>
    <input id="<%- idPrefix %>-userTime" class="form-control" type="time" name="userTime" required>
  </div>
</div>
<div class="form-group">
  <label for="<%- idPrefix %>-cargo" class="control-label is-required"><%- t('transportOrders', 'cargo:carWash') %></label>
  <textarea id="<%- idPrefix %>-cargo" class="form-control" name="cargo" rows="2" required></textarea>
</div>
<div class="row">
  <div class="col-md-6 form-group">
    <label for="<%- idPrefix %>-fromAddress" class="control-label is-required"><%- t('transportOrders', 'fromAddress:carWash') %></label>
    <textarea id="<%- idPrefix %>-fromAddress" class="form-control" name="fromAddress" rows="3" required></textarea>
  </div>
  <div class="col-md-6 form-group">
    <label for="<%- idPrefix %>-toAddress" class="control-label"><%- t('transportOrders', 'toAddress:carWash') %></label>
    <textarea id="<%- idPrefix %>-toAddress" class="form-control" name="toAddress" rows="3"></textarea>
  </div>
</div>
<% include _costsAndNotes.ejs %>
