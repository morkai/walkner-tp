<tr class="list-item <%- row.className ? row.className : '' %> <%- row.changed.all ? 'is-changed' : '' %>" data-id="<%- row._id %>">
  <td class="is-number"><%- row.rid %></td>
  <td><%- t('transportOrders', 'kind:' + row.kind) %></td>
  <td>
    <i class="tp-confirm-indicator fa fa-<%- row.ownerConfirmed ? 'check' : 'times' %> <%- row.changed.ownerConfirmed ? 'is-changed' : '' %>"></i>
    <span class="<%- row.changed.owner || row.changed.tel ? 'is-changed' : '' %>"><%- row.owner %></span>
  </td>
  <td class="is-min <%- row.changed.userDate ? 'is-changed' : '' %>"><%- row.userDate %></td>
  <td class="is-min <%- row.changed.userDate ? 'is-changed' : '' %>"><%- row.userTime %></td>
  <td class="is-min is-number <%- row.changed.quantity ? 'is-changed' : '' %>"><%- row.quantity %></td>
  <td class="is-min <%- row.changed.unit ? 'is-changed' : '' %>"><%- row.unit %></td>
  <td class="<%- row.changed.fromAddress ? 'is-changed' : '' %>"><%- row.fromAddress %></td>
  <td class="<%- row.changed.toAddress ? 'is-changed' : '' %>"><%- row.toAddress %></td>
  <td class="<%- row.changed.symbol ? 'is-changed' : '' %>"><%- row.symbol %></td>
  <td class="is-min <%- row.changed.driverDate ? 'is-changed' : '' %>"><%- row.driverDate %></td>
  <td class="is-min <%- row.changed.driverDate ? 'is-changed' : '' %>"><%- row.driverTime %></td>
  <td>
    <i class="tp-confirm-indicator fa fa-<%- row.driverConfirmed ? 'check' : 'times' %> <%- row.changed.driverConfirmed ? 'is-changed' : '' %>"></i>
    <span class="<%- row.changed.driver ? 'is-changed' : '' %>"><%- row.driver %></span>
  </td>
  <td class="is-min is-number <%- row.changed.km ? 'is-changed' : '' %>"><%- row.km %></td>
  <td class="is-min is-number <%- row.changed.hours ? 'is-changed' : '' %>"><%- row.hours %></td>
  <td class="is-min is-number <%- row.changed.price ? 'is-changed' : '' %>"><%- row.price %></td>
  <td>
    <i class="tp-confirm-indicator fa fa-<%- row.dispatcherConfirmed ? 'check' : 'times' %> <%- row.changed.dispatcherConfirmed ? 'is-changed' : '' %>"></i>
    <span class="<%- row.changed.dispatcher ? 'is-changed' : '' %>"><%- row.dispatcher %></span>
  </td>
  <td class="actions">
    <% row.actions.forEach(function(action) { %>
    <a href="<%- action.href %>" class="btn btn-<%- action.type || 'default' %> action-<%- action.id %>" title="<%= action.label %>"><i class="fa fa-<%- action.icon %>"></i></a>
    <% }) %>
  </td>
</tr>
