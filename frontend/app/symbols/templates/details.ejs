<div class="panel panel-primary">
  <div class="panel-heading">
    <%- t('symbols', 'PANEL:TITLE:details') %>
  </div>
  <div class="panel-details">
    <div class="props first">
      <div class="prop">
        <div class="prop-name"><%- t('symbols', 'PROPERTY:_id') %></div>
        <div class="prop-value"><%= model._id %></div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('symbols', 'PROPERTY:group') %></div>
        <div class="prop-value"><%= model.group || '-' %></div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('symbols', 'PROPERTY:name') %></div>
        <div class="prop-value"><%- model.name || '-' %></div>
      </div>
    </div>
  </div>
</div>
