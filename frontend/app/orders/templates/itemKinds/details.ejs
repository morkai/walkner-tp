<div class="panel panel-<%- item.panelType %> orderItems-details">
  <div class="btn-toolbar">
    <button id="<%- idPrefix %>-toggle" class="btn btn-default"><i class="fa fa-chevron-up"></i></button>
    <span class="orderItems-details-summary"><%- item.rid %>: <%- item.kindText %></span>
    <button id="<%- idPrefix %>-confirm" class="btn btn-default orderItems-details-action"><i class="fa fa-thumbs-o-up"></i><span><%- t('orders', 'item:confirm') %></span></button>
    <button id="<%- idPrefix %>-edit" class="btn btn-default orderItems-details-action"><i class="fa fa-edit"></i><span><%- t('orders', 'item:edit') %></span></button>
    <button id="<%- idPrefix %>-delete" class="btn btn-default orderItems-details-action"><i class="fa fa-times"></i><span><%- t('orders', 'item:delete') %></span></button>
  </div>
  <div class="panel-details">
    <div class="row">
      <div class="col-md-4">
        <div class="props first">
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'PROPERTY:rid') %></div>
            <div class="prop-value"><%- item.rid %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'PROPERTY:kind') %></div>
            <div class="prop-value"><%= item.kindText %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'PROPERTY:userDateTime') %></div>
            <div class="prop-value"><%= item.userDate %>, <%= item.userTime %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'PROPERTY:quantity') %> [<%= item.unit %>]</div>
            <div class="prop-value"><%= item.quantity %></div>
          </div>
          <% if (item.data && item.data.passengers) { %>
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'data:passengers') %></div>
            <div class="prop-value"><pre><%= item.data.passengers %></pre></div>
          </div>
          <% } %>
          <% if (item.data && item.data.flightNo) { %>
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'data:flightNo') %></div>
            <div class="prop-value"><%= item.data.flightNo %></div>
          </div>
          <% } %>
          <% if (item.data && item.data.airport) { %>
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'data:airport') %></div>
            <div class="prop-value"><%= item.data.airport %></div>
          </div>
          <% } %>
          <% if (item.data && item.data.vehicle) { %>
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'data:vehicle') %></div>
            <div class="prop-value"><pre><%= item.data.vehicle %></pre></div>
          </div>
          <% } %>
        </div>
      </div>
      <div class="col-md-4">
        <div class="props">
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'PROPERTY:symbol') %></div>
            <div class="prop-value"><%= item.symbol || '-' %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'PROPERTY:fromAddress') %></div>
            <div class="prop-value">
              <pre><%= item.fromAddress %></pre>
            </div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'PROPERTY:toAddress') %></div>
            <div class="prop-value">
              <pre><%= item.toAddress || '-' %></pre>
            </div>
          </div>
        </div>
      </div>
      <div class="col-md-4">
        <div class="props">
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'PROPERTY:confirmed') %></div>
            <div class="prop-value">
              <span class="orderItems-confirmed is-<%- item.ownerConfirmed %>" title="<%- t('orders', 'PROPERTY:ownerConfirmed') %>"><%- t('orders', 'confirmed:owner') %></span>
              <span class="orderItems-confirmed is-<%- item.dispatcherConfirmed %>" title="<%- t('orders', 'PROPERTY:dispatcherConfirmed') %>"><%- t('orders', 'confirmed:dispatcher') %></span>
              <span class="orderItems-confirmed is-<%- item.driverConfirmed %>" title="<%- t('orders', 'PROPERTY:driverConfirmed') %>"><%- t('orders', 'confirmed:driver') %></span>
            </div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'PROPERTY:driver') %></div>
            <div class="prop-value"><%- item.driver %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'PROPERTY:driverDateTime') %></div>
            <div class="prop-value">
              <% if (item.driverDate) { %>
              <%= item.driverDate %>, <%- item.driverTime %>
              <% } else { %>
              -
              <% } %>
            </div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'PROPERTY:km') %></div>
            <div class="prop-value"><%= item.km %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'PROPERTY:hours') %></div>
            <div class="prop-value"><%= item.hours %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('orders', 'PROPERTY:price') %></div>
            <div class="prop-value"><%= item.price %></div>
          </div>
        </div>
      </div>
    </div>
    <% if (item.notes) { %>
    <pre><%= item.notes %></pre>
    <% } %>
  </div>
</div>
